    <DataSet Name="BAQReportResult">
      <Query>
        <DataSourceName>dsBAQReport</DataSourceName>
        <CommandText>="SELECT [ShipHead_Company],[ShipHead_ShipDate],[Part_PartNum],[Part_PartDescription],[Calculated_ShippedQty],[RefCategory_RefCategory],[OrderDtl_UnitPrice],[Customer_CustID],[Customer_Name],[Customer_State],[ShipTo_Name],[ShipTo_City],[ShipTo_State],[Calculated_Revenue],[Calculated_Year],[Calculated_Month],[Calculated_InvcEOM],[ShipTo_ShipToNum],[Part_ProdCode],[InvcHead_InvoiceDate],[InvcDtl_InvoiceNum],[InvcDtl_SellingShipQty],[InvcDtl_UnitPrice] 
, rp.Date01 As FromDate, rp.Date02 As ToDate
, rp.Check01 As IncludeSales, rp.Check02 As IncludeZeroPrice, rp.Check03 As IncludeNonStock, rp.SortBy As SortBy, rp.Number01 

FROM dbo.[BAQReportResult_" + Parameters!TableGuid.Value + "] rr
	Inner Join dbo.[BAQReportParameter_" + Parameters!TableGuid.Value + "] rp on rr.[ShipHead_Company] = rp.[Cur-Comp] 
	where rr.[InvcHead_InvoiceDate] &gt;= rp.Date01 AND rr.[InvcHead_InvoiceDate] &lt;= rp.Date02
"</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
