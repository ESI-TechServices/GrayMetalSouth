  <Code>Public Shared Function FindFilter(filter2Value As String, filterName As String) As String
        Dim filterValue As String = String.Empty

        If (filter2Value.ToLower().Contains("&lt;" + filterName.ToLower() + "&gt;")) Then

            Dim startPos As Integer = filter2Value.ToLower().IndexOf("&lt;" + filterName.ToLower() + "&gt;")
            startPos = startPos + filterName.Length + 2
            Dim endPos As Integer = filter2Value.ToLower().IndexOf("&lt;/" + filterName.ToLower() + "&gt;")
            Dim charCount As Integer = endPos - startPos
            If (charCount &gt; 0 And filter2Value.Length &gt; startPos + charCount) Then

                filterValue = filter2Value.Substring(startPos, charCount)

            End If

        End If

        Return filterValue

    End Function
</Code>
